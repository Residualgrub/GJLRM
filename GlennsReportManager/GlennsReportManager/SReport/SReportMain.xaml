<Window x:Class="GlennsReportManager.SReport.SReportMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GlennsReportManager.SReport"
        mc:Ignorable="d"
        Title="SReportMain" Height="450" Width="800">

    <Border Padding="10">
        <StackPanel>
            <Grid Background="#2c99ac">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>

                <TextBlock Margin="10 0 0 0" Text="Reports from " Grid.Column="0" Foreground="#ffffff"/>
                
            </Grid>
        </StackPanel>
    </Border>
    
</Window>
