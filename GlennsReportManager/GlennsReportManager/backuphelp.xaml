<Window x:Class="GlennsReportManager.backuphelp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GlennsReportManager"
        mc:Ignorable="d"
        Title="Backup:Help" Height="450" Width="800" Icon=".\Icons\information.ico" ResizeMode="NoResize" SizeToContent="Height">
    <Border Padding="10">
        <StackPanel Orientation="Vertical">
            <TextBlock Text="Welcome to the backup service help. What would you like to know about?" HorizontalAlignment="Center" FontSize="25" TextWrapping="Wrap" TextAlignment="Center" FontWeight="Bold"/>
            <Rectangle></Rectangle>
        </StackPanel>
        
    </Border>
</Window>
